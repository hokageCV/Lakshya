{
    "manifest_version": 3,
    "name": "Varsh",
    "version": "0.0.1",
    "description": "See the year in new tab",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "icons":{
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "host_permissions": ["<all_urls>"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "background":{
        "service_workers": "background.js"
    }
  }